{
  "presets": [
    ["env", { 
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      },
      "loose": true 
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "add-module-exports"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
